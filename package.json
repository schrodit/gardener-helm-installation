    "@types/node-forge": "^1.0.0",
    "axios": "^0.26.0",
    "commander": "^9.0.0",
    "extract-zip": "^2.0.1",
    "ip-cidr": "^3.0.4",
    "logform": "^2.4.2",
    "node-forge": "^1.2.1",
    "semver": "^7.3.5",
    "validator": "^13.7.0",
    "which": "^2.0.2",
    "winston": "^3.6.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@types/argparse": "^2.0.10",
