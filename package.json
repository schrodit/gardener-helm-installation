    "@types/node": "^17.0.21",
    "@types/semver": "^7.3.9",
    "@types/validator": "^13.7.1",
    "@types/winston": "^2.4.4",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "create-ts-index": "^1.14.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
