  "types": "lib/index.d.ts",
  "repository": "git@github.com:schrodit/gardener-installation.git",
  "author": "schrodit <mail@timschrodi.tech>",
  "license": "MIT",
  "scripts": {
    "setup": "node --loader ts-node/esm ./index.ts",
    "lintFix": "eslint '**/*.ts' --quiet --fix",
    "lint": "eslint '**/*.ts' --quiet",
    "test": "jest --passWithNoTests",
    "build": "cti create ./src/ts -e 'versions' -e '__tests__' -e '__test__' -i '*.test.ts' && tsc",
    "prepack": "yarn build",
    "cleanIndex": "rm -f ./src/**/index.ts && rm -f ./src/**/index.ts.bak",
    "clean": "yarn run cleanIndex && rm -rf ./node_modules && rm -rf ./lib"
  },
  "dependencies": {
